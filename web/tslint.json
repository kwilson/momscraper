{
    "extends": [
      "tslint:recommended",
      "tslint-react"
    ],
    "rules": {
        "jsx-no-multiline-js": false,
        "no-submodule-imports": [
            true,
            "rxjs"
        ],
        "trailing-comma": false,
        "quotemark": [
            true,
            "single",
            "jsx-double"
          ],
          "object-literal-sort-keys": true
    }
}
